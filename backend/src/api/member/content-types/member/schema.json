{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "singularName": "member",
    "pluralName": "members",
    "displayName": "Member",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "FirstName": {
      "type": "string",
      "required": true
    },
    "LastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "Role": {
      "type": "enumeration",
      "enum": [
        "President",
        "Vice President",
        "Treasurer",
        "Secretary",
        "Member",
        "Alumni",
        "Faculty Advisor",
        "Officer",
        "Other",
        "Event Coordinator"
      ],
      "required": true,
      "default": "Member"
    },
    "JoinDate": {
      "type": "date",
      "required": true
    },
    "GraduationYear": {
      "type": "integer"
    },
    "club": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::club.club",
      "inversedBy": "members"
    },
    "profilePicture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "skills": {
      "type": "json"
    },
    "links": {
      "type": "json"
    }
  }
}
