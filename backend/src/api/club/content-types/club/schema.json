{
  "kind": "collectionType",
  "collectionName": "clubs",
  "info": {
    "singularName": "club",
    "pluralName": "clubs",
    "displayName": "Club"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Description": {
      "type": "blocks",
      "required": true
    },
    "President": {
      "type": "string",
      "required": true
    },
    "contactEmail": {
      "type": "email",
      "required": true
    },
    "foundedYear": {
      "type": "integer"
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "club"
    },
    "logo": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "socialMediaLinks": {
      "type": "json"
    },
    "members": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::member.member",
      "mappedBy": "club"
    }
  }
}
